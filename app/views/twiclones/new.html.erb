<%= form_with(model: @twiclone, local: true) do |form| %>
   <div class ="content">
     <%= form.label :"つぶやく" %>
     <%= form.text_field :content %>
   </div>
   <%= form.submit %>
 <% end %>

 <table>
  <% @twiclones.each do |twiclone| %>
  <tr>
    <td><%= twiclone.content %></td>
  </tr>
  <% end %>
</table>
